<div class="table-responsive">
  <h1>List of Policies</h1>
  <table class="table-striped table-condensed table-bordered">
    <tr>
      <th>Name</th>
      <th>arn</th>
    </tr>
    <% @policies.each do |policy| %>
      <tr>
        <td><%= policy.policy_name %></td>
        <td><%= policy.policy_arn %></td>
      </tr>
    <% end %>
  </table>
</div>